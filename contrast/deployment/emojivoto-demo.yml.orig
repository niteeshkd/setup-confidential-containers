metadata:
  name: emoji-svc
spec:
  ports:
  - name: grpc
    port: 8080
    targetPort: 8080
  - name: prom
    port: 8801
    targetPort: 8801
  selector:
    app.kubernetes.io/name: emoji-svc
---
apiVersion: v1
