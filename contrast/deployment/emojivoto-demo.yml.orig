    app.kubernetes.io/part-of: emojivoto
    app.kubernetes.io/version: v11
  name: vote-bot
spec:
  replicas: 1
  selector:
    matchLabels:
      app.kubernetes.io/name: vote-bot
      version: v11
  template:
    metadata:
      labels:
        app.kubernetes.io/name: vote-bot
        version: v11
    spec:
      containers:
      - command:
        - emojivoto-vote-bot
        env:
        - name: WEB_HOST
          value: web-svc:443
        image: ghcr.io/3u13r/emojivoto-web:coco-1
        name: vote-bot
        resources:
          limits:
            memory: 25Mi
          requests:
            memory: 25Mi
